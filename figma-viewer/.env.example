# Supabase Configuration
# Copy this file to .env and fill in your actual values
# Never commit .env file to Git!

# Supabase Project URL (found in Supabase Dashboard → Settings → API)
VITE_SUPABASE_URL=https://your-project-id.supabase.co

# Supabase Anon/Public Key (found in Supabase Dashboard → Settings → API)
# This is the public key that is safe to expose in client-side code
# Make sure RLS (Row Level Security) policies are properly configured!
VITE_SUPABASE_ANON_KEY=your_anon_key_here

# Session inactivity timeout in seconds for viewer (used to auto-close
# long-idle prototype sessions on the client side). Keep in sync with
# the same variable in figma-analytics.
VITE_SESSION_INACTIVITY_TIMEOUT_SECONDS=300
